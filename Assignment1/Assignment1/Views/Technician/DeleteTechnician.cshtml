@model Technician
@{
    ViewBag.Title = "Delete Technician";
}
<h2>Confirm Deletion</h2>
<h3>@Model.TechnicianName</h3>
<form asp-controller="Technician" asp-action="Delete" method="post">
    <input type="hidden" asp-for="TechnicianId" />

    <button type="submit" class="btn btn-primary">Delete</button>
    <a asp-controller="Technician" asp-action="ManageTechnician" class="btn btn-primary">Cancel</a>
</form>