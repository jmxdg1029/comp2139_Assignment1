@model Technician
@{
    string title = "Edit Technician";
}
<h2>@title</h2>

<form asp-action="EditTechnician" method="post">
    <div asp-validation-summary="All" text-danger></div>
    <div class="form-group">
        <label asp-for="TechnicianName">Name</label>
        <input asp-for="TechnicianName" class="form-control">
    </div>

    <div class="form-group">
        <label asp-for="TechnicianEmail">Email</label>
        <input asp-for="TechnicianEmail" class="form-control">
    </div>

    <div class="form-group">
        <label asp-for="TechnicianPhone">Phone</label>
        <input asp-for="TechnicianPhone" class="form-control">
    </div>

    <input type="hidden" asp-for="TechnicianId" />

    <button type="submit" class="btn btn-primary">Edit</button>
    <a asp-controller="Technician" asp-action="ManageTechnician" class="btn btn-primary">Cancel</a>
</form>